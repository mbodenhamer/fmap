all: test

#-------------------------------------------------------------------------------

test:
	nosetests -v

dbtest:
	nosetests -v --ipdb --ipdb-failures

.PHONY: test dbtest
#-------------------------------------------------------------------------------


clean: 
	@rm -f *.py[co] */*.py[co] */*/*.py[co]
	@rm -rf */__pycache__ */*/__pycache__

.PHONY: clean
#-------------------------------------------------------------------------------
